@model PlaceViewModel

@{
    ViewData["Title"] = "Create Place";
}

<h2>Create a new Place</h2>

<form asp-action="Create">
    <div>
        <label asp-for="Name"></label>
        <input asp-for="Name" required />
    </div>

    <div>
        <button type="submit">Create</button>
    </div>
</form>
